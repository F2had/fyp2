<%- include('partials/head.ejs') %>

<body id="page-top " class="d-flex flex-column min-vh-100">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <!-- Sidebar -->
    <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

      <!-- Sidebar - Brand -->
      <a class="sidebar-brand d-flex align-items-center justify-content-center" href="#">
        <div class="sidebar-brand-text mx-3">
          <img src="/imgs/logo.png" width="85" height="85" alt="">
        </div>
      </a>

      <!-- Divider -->
      <hr class="sidebar-divider my-0">

      <!-- Nav Item - Dashboard -->
      <li class="nav-item active">
        <a class="nav-link" href="/panel">
          <i class="fas fa-fw fa-tachometer-alt"></i>
          <span>Dashboard</span></a>
      </li>

      <!-- Divider -->
      <hr class="sidebar-divider">

      <!-- Sidebar Toggler (Sidebar) -->
      <div class="text-center d-none d-md-inline">
        <button class="rounded-circle border-0" id="sidebarToggle"></button>
      </div>

      <hr class="sidebar-divider d-none d-md-block">

      <!-- Logout -->
      <div>
        <li class="nav-item">
          <a class="nav-link text-center fixed-bottom" data-toggle="modal" data-target="#logoutModal" href="#">
            Logout
          </a>
        </li>
      </div>
    </ul>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <!-- Topbar -->
        <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

          <!-- Sidebar Toggle (Topbar) -->
          <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
            <i class="fa fa-bars"></i>
          </button>

          <!-- Topbar Navbar -->
          <ul class="navbar-nav ml-auto">

            <div class="topbar-divider d-none d-sm-block"></div>

            <!-- Nav Item - User Information -->
            <li class="nav-item dropdown arrow">
              <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <span class="mr-2 d-none d-lg-inline text-gray-600 small">
                  <%= user.name %>
                  <i class="fas fa-user"></i>
                </span>

              </a>
              <!-- Dropdown - User Information -->
              <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in" aria-labelledby="userDropdown">
                <a class="dropdown-item" href="/panel/profile">
                  <i class="fas fa-user fa-sm fa-fw mr-2 text-gray-400"></i>
                  Profile
                </a>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item" href="#" data-toggle="modal" data-target="#logoutModal">
                  <i class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-400"></i>
                  Logout
                </a>
              </div>
            </li>

          </ul>

        </nav>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid ">



          <% if (success.length ) { %>
          <hr>

          <% success.forEach(success=> { %>
          <div class="alert alert-success alert-dismissible fade show" role="alert">
            <%= success %>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <% }) %>
          <hr>

          <% } %>

          <% if (error.length ) { %>
          <hr>

          <% error.forEach(error=> { %>
          <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <%= error %>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <% }) %>
          <hr>

          <% } %>


          <div class="row">
            <div class="col">
              <h5>Student: <%= slot['student'].name %> </h5>
            </div>
            <div class="w-100"></div>
            <div class="col">
              <h5>Title: <%= slot['student'].title %> </h5>
            </div>

            <div class="w-100"></div>

            <div class="col">
              <h5>FYP: <%= slot['student'].fyp %> </h5>
            </div>

          </div>

          <div class="w-100 m-4"></div>

          <div class="container-fluid">


            <h4 class="bold">Assessment</h4>

            <div class="w-100 "></div>



            <form action="/feedback/2" class="" method="post">
              <div class="row justify-content-between">
                <input type="hidden" value="<%= csrfToken %>" name="_csrf"/>
                <input type="hidden" value="<%= user.id %>" name="panelID"/>
                <input type="hidden" value="<%= slot.id %>" name="slotID"/>
                <div class="col-6">
                  <div class="form-group">
                  
                    <select id="fulfillment_of_objective" name="fulfillment_of_objectives" class="custom-select">
                      <option value="Objectives" disabled selected>Fulfillment of Objectives</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select name="fulfillment_of_requirements_specifications" class="custom-select">
                      <option value="Objectives" disabled selected>Fulfillment of Requirements/Specifications</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select name="design_and_technical_implementation" class="custom-select">
                      <option value="Objectives" disabled selected>Design and Technical Implementation</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select  name="ui_ux" class="custom-select">
                      <option value="Objectives" disabled selected>UI/UX</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select  name="system_complexity" class="custom-select">
                      <option value="Objectives" disabled selected>System Complexity</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select  name="error_free_and_error_handling" class="custom-select">
                      <option value="Objectives" disabled selected>Error-free and Error Handling</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>


                <div class="col">
                  <div class="form-group">
                    <select id="objectives" name="testing_techniques" class="custom-select">
                      <option value="Objectives" disabled selected>Testing Techniques</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select id="stakeholder" name="stakeholder" class="custom-select">
                      <option value="Objectives" disabled selected>Collaboration with Stakeholder</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="No evidence of collaboration">No evidence of collaboration</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select id="p_skills" name="p_skills" class="custom-select">
                      <option value="" disabled selected>Presentation Skills</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <div class="form-group">
                    <select id="overall" name="overall" class="custom-select">
                      <option value="overall" disabled selected>Overall Presentation</option>
                      <option value="Satisfactory">Satisfactory</option>
                      <option value="Unsatisfactory">Unsatisfactory</option>
                      <option value="Not Applicable">Not Applicable</option>
                    </select>
                  </div>
                </div>

                <div class="w-100 "></div>

                <div class="col">
                  <h4>Comments</h4>
                </div>

                <div class="w-100 "></div>

                <div class="col form-group">
                  <input type="hidden" name="comments"  id="comments">

                  <div id="editor">

                  </div>


                </div>

                <div class="w-100 m-4"></div>


                <div class="col-4">
                  <button class="btn btn-outline-primary float-none feedback" type="submit">Submit</button>
                </div>


              </div>
            </form>



          </div>



        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      <%- include('partials/footer.ejs') %>

      <!-- Sweet Alert -->
      <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

      <!-- quilljs -->
      <script src="https://cdn.quilljs.com/1.3.6/quill.js"></script>





      <script src="/js/feedback.js"></script>

      <%- include('partials/bodyEnd.ejs') %>